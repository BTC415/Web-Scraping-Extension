{
  "manifest_version": 2,
  "version": "1.0.0",
  "name": "Next.js Data Viewer",
  "description": "Displays the Next.js or React data cached on the active tab.",
  "browser_action": {
    "default_icon": {
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
